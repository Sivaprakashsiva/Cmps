package excer;

import java.util.Scanner;

public class Prime {
	public static void main(String[] args) {
		//Scanner scanner=new Scanner(System.in);
		System.out.println("hi");
		for(int i=2;i<=100;i++){
			int count=0;
			for(int j=1;j<=i;j++){
				if(i%j==0){
					count++;
				}
			}
			if(count==0){
				System.out.println(i);
			}
		}
	}

}
