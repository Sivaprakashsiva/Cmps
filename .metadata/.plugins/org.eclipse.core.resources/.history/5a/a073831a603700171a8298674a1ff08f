package excer;

import java.util.Scanner;

public class Prime {
	public static void main(String[] args) {
		//Scanner scanner=new Scanner(System.in);
		System.out.println("hi");
		for(int i=2;i<=100;i++){
			int count=0;
			for(int j=i;j>=1;j--){
				if(i%j==0){
					count++;
					
				}	
			}
			if(count==0){
				System.out.print(i);
			}
			else{
				System.out.print("not\n");
			}
		}
	}

}
