package ex2;

public class DisplayShapes {
public static void main(String[] args) {
	System.out.println("*********\t    ****   \t  *  \t    *");
	System.out.println("*       *\t *	  * \t *** \t   * *");
	System.out.println("*       *\t*	   *\t*****\t  *   *");
	System.out.println("*       *\t*	   *\t  *  \t *     *");
	System.out.println("*       *\t*	   *\t  *  \t*       *");
	System.out.println("*       *\t*	   *\t  *  \t *     *");
	System.out.println("*       *\t *	  *\t  *  \t  *   *");
	System.out.println("*********\t    ****  \t  *  \t    *");
	
}
}

